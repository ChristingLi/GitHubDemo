<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="rightPadding" format="dimension" />
    <attr name="drawerType" format="boolean" />
    <declare-styleable name="SlidingMenu">
        <attr name="rightPadding" />
        <attr name="drawerType" />
    </declare-styleable>
    <declare-styleable name="DragMenuLayout">
        <attr name="dragRange" format="float" />
        <attr name="style">
            <enum name="Translate" value="0" />
            <enum name="Scale" value="1" />
        </attr>
    </declare-styleable>
</resources>